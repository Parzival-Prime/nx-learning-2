{
  "name": "seller-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/seller-ui",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project seller-ui --web",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "root": "apps/seller-ui",
        "outputPath": "dist/apps/seller-ui",
        "assets": ["public"]
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/seller-ui"
        },
        "production": {
          "outputPath": "dist/apps/seller-ui"
        }
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "seller-ui:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "seller-ui:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "seller-ui:build:production",
          "dev": false
        }
      }
    }
  }
}
